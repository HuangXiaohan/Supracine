";\n        fetch(source)\n            .then(res => res.json())\n            .then(\n                (result) => {\n                    if(result.Response === \"False\"){\n                        this.setState({\n                            movieList: [],\n                            totalResults: 0\n                        },()=>alert(\"Error! \"+ result.Error));\n                    }\n                    this.setState({\n                        movieList: result.Search,\n                        totalResults: result.totalResults\n                    });\n                },\n                (error) => {\n                    this.setState({\n                        movieList: [],\n                        totalResults: 0\n                    },()=>alert(\"Error! \"+ error));\n                }\n            );\n    }\n\n    handleChange(e){\n        let {name: fieldName, value} = e.target;\n\n        this.setState({\n            [fieldName]: value\n        });\n    }\n}\n\nexport default withRouter(SearchMovie);","/home/huang/Documents/Supracine/src/MovieList.js",[],"/home/huang/Documents/Supracine/src/PageOption.js",[],"/home/huang/Documents/Supracine/src/MovieCard.js",[],"/home/huang/Documents/Supracine/src/MovieDetail.js",[],{"ruleId":"48","replacedBy":"49"},{"ruleId":"50","replacedBy":"51"},{"ruleId":"52","severity":1,"message":"53","line":5,"column":10,"nodeType":"54","messageId":"55","endLine":5,"endColumn":15},"no-native-reassign",["56"],"no-negated-in-lhs",["57"],"no-unused-vars","'Route' is defined but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]